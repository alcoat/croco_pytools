name: croco_pyenv
channels:
  - conda-forge
  - defaults
dependencies:
  - python=3.9
  - setuptools>60,<=74
  - numpy<=1.23.5
  - matplotlib-base
  - cartopy
  - scipy
  - wxpython
  - traits
  - traitsui
  - netcdf4
  - xarray
  - dask
  - geopandas
  - regionmask
  - pyinterp
  - gcc>=8.3.0
  - gfortran>=8.3.0
  - netcdf-fortran=4.6.0
  - cdsapi
  - pyqt
  - pip
  - pip:
      - copernicusmarine>=1.1.0,<2.0.0
